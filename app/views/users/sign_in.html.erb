<h1>登入</h1>

<%= form_with(url: sessions_path , scope: "user", method: "POST") do |form| %>

<div>
    <%= form.label :account, "帳號" %>
    <%= form.text_field :account, autocomplete:"off", placeholder:"使用 Email 或 Username 登入" %>
</div>

<div>
    <%= form.label :password, "密碼" %>
    <%= form.password_field :password, autocomplete:"off" %>
</div>

<%= form.submit %>

<% end %>